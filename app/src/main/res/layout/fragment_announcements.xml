<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="true"
    android:focusable="true"
    android:background="@drawable/background1_complete"
    tools:context=".AnnouncementsFragment">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/announcements_fragment_temp_text_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Announcements Fragment"
        android:textSize="36sp"
        android:textColor="@color/game_text_color"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>